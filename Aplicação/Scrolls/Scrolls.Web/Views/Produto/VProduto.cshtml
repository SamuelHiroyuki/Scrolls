<!-- SEE TOO -->
<section id="st">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="section-title">
                    <h4 class="title">Veja Também</h4>
                    <div class="float-right">
                        <div class="product-slick-dots-3 custom-dots"></div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Title -->

        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div id="product-slick-3" class="product-slick">
                    @foreach (var n in ViewBag.Rep)
                    {
                        <!-- Product Single -->
                        <div class="product product-single product-hot">
                            <div class="product-thumb">
                                <div class="product-label">
                                    @if (n.Promocao != null)
                                    {
                                        <span class="sale">@n.Promocao%</span>
                                    }
                                    <span class="replace"></span>
                                </div>
                                <img src='"https://manager-scrolls.azurewebsites.net/Imagens/Produtos/" + "@n.Imagem1"' alt="">
                            </div>
                            <div class="product-body">
                                @if (n.Promocao != null)
                                {
                                    <h5 class="product-price">R$ @Math.Round(Convert.ToDouble(n.Preco - ((n.Preco * n.Promocao) / 100)), 2) <del class="product-old-price">R$ @n.Preco</del></h5>
                                }
                                else
                                {
                                    <h5 class="product-price">R$ @n.Preco</h5>
                                }
                                <p class="product-name"><a href="#">@n.Nome</a></p>
                                <div class="product-rating">
                                    <i class="icon ion-md-star"></i>
                                    <i class="icon ion-md-star"></i>
                                    <i class="icon ion-md-star"></i>
                                    <i class="icon ion-md-star"></i>
                                    <i class="icon ion-md-star-outline empty"></i>
                                </div>
                                <form action="" class="product-btns d-flex justify-content-center">
                                    <button class="primary-btn add-to-cart" type="button" onclick="window.location.href='@Url.Action("AddCart", "Home", new { id = n.Id })'"><i class="icon ion-md-cart mr-1"></i>Add ao Carinho</button>
                                </form>
                            </div>
                        </div>
                        <!-- /Product Single -->
                    }
                    <!-- /Product Single -->
                </div><!-- Product-Slick -->
            </div><!-- grid -->
        </div><!-- row -->
    </div><!-- container -->
</section>
<!-- /SEE TOO -->
