

@{
    ViewBag.Title = ViewBag.P.Nome;

    ViewBag.Active = "show-on-click";
}

<!-- PRODUCT VIEW -->
<section class="section">
    <!-- container -->
    <div class="container">
        <!-- row -->
        <div class="row">
            <!--  Product Details -->
            <div class="product product-details col-md-12 d-md-flex justify-content-md-between align-items-md-center">
                <div class="col-md-5 col-12">
                    <div class="product-view item">
                        <img src="@ViewBag.Imagem1" alt="">
                    </div>
                </div>
                <div class="col-md-6 col-12">
                    <div class="product-body ">
                        <div class="product-label">
                            @if (ViewBag.P.Promocao != null)
                            {
                                <span class="sale">-@ViewBag.P.Promocao%</span>
                            }
                            @if (ViewBag.P.Quantidade <= 0)
                            {

                                <span class="replace">Esgotado</span>
                            }
                        </div>
                        <h4 class="product-name">@ViewBag.P.Nome</h4>
                        @if (ViewBag.P.Promocao != null)
                        {
                            <h5 class="product-price">R$ @Math.Round(Convert.ToDouble(ViewBag.P.Preco - ((ViewBag.P.Preco * ViewBag.P.Promocao) / 100)), 2) <del class="product-old-price">R$ @ViewBag.P.Preco</del></h5>
                        }
                        else
                        {
                            <h5 class="product-price">R$ @ViewBag.P.Preco</h5>
                        }
                        <div class="d-flex">
                            <p class="mr-2">CÓD.:(@ViewBag.P.Id)</p>
                            @{ if (ViewBag.notas == 0)
                                {
                                    <div class="product-rating" style="font-size: 15px">
                                        <i class="icon ion-md-star-outline empty"></i>
                                        <i class="icon ion-md-star-outline empty"></i>
                                        <i class="icon ion-md-star-outline empty"></i>
                                        <i class="icon ion-md-star-outline empty"></i>
                                        <i class="icon ion-md-star-outline empty"></i>
                                    </div><!-- Rate -->
                                }
                                else
                                {
                                    if (ViewBag.notas > 0 && ViewBag.notas < 1)
                                    {
                                        <div class="product-rating" style="font-size: 15px">
                                            <i class="icon ion-md-star-half"></i>
                                            <i class="icon ion-md-star-outline empty"></i>
                                            <i class="icon ion-md-star-outline empty"></i>
                                            <i class="icon ion-md-star-outline empty"></i>
                                            <i class="icon ion-md-star-outline empty"></i>
                                        </div><!-- Rate -->
                                    }
                                    else
                                    {
                                        if (ViewBag.notas == 1)
                                        {
                                            <div class="product-rating" style="font-size: 15px">
                                                <i class="icon ion-md-star"></i>
                                                <i class="icon ion-md-star-outline empty"></i>
                                                <i class="icon ion-md-star-outline empty"></i>
                                                <i class="icon ion-md-star-outline empty"></i>
                                                <i class="icon ion-md-star-outline empty"></i>
                                            </div><!-- Rate -->
                                        }
                                        else
                                        {
                                            if (ViewBag.notas > 1 && ViewBag.notas < 2)
                                            {
                                                <div class="product-rating" style="font-size: 15px">
                                                    <i class="icon ion-md-star"></i>
                                                    <i class="icon ion-md-star-half"></i>
                                                    <i class="icon ion-md-star-outline empty"></i>
                                                    <i class="icon ion-md-star-outline empty"></i>
                                                    <i class="icon ion-md-star-outline empty"></i>
                                                </div><!-- Rate -->
                                            }
                                            else
                                            {
                                                if (ViewBag.notas == 2)
                                                {
                                                    <div class="product-rating" style="font-size: 15px">
                                                        <i class="icon ion-md-star"></i>
                                                        <i class="icon ion-md-star"></i>
                                                        <i class="icon ion-md-star-outline empty"></i>
                                                        <i class="icon ion-md-star-outline empty"></i>
                                                        <i class="icon ion-md-star-outline empty"></i>
                                                    </div><!-- Rate -->
                                                }
                                                else
                                                {
                                                    if (ViewBag.notas > 2 && ViewBag.notas < 3)
                                                    {
                                                        <div class="product-rating" style="font-size: 15px">
                                                            <i class="icon ion-md-star"></i>
                                                            <i class="icon ion-md-star"></i>
                                                            <i class="icon ion-md-star-half"></i>
                                                            <i class="icon ion-md-star-outline empty"></i>
                                                            <i class="icon ion-md-star-outline empty"></i>
                                                        </div><!-- Rate -->
                                                    }
                                                    else
                                                    {
                                                        if (ViewBag.notas == 3)
                                                        {
                                                            <div class="product-rating" style="font-size: 15px">
                                                                <i class="icon ion-md-star"></i>
                                                                <i class="icon ion-md-star"></i>
                                                                <i class="icon ion-md-star"></i>
                                                                <i class="icon ion-md-star-outline empty"></i>
                                                                <i class="icon ion-md-star-outline empty"></i>
                                                            </div><!-- Rate -->
                                                        }
                                                        else
                                                        {
                                                            if (ViewBag.notas > 3 && ViewBag.notas < 4)
                                                            {
                                                                <div class="product-rating" style="font-size: 15px">
                                                                    <i class="icon ion-md-star"></i>
                                                                    <i class="icon ion-md-star"></i>
                                                                    <i class="icon ion-md-star"></i>
                                                                    <i class="icon ion-md-star-half"></i>
                                                                    <i class="icon ion-md-star-outline empty"></i>
                                                                </div><!-- Rate -->
                                                            }
                                                            else
                                                            {
                                                                if (ViewBag.notas == 4)
                                                                {
                                                                    <div class="product-rating" style="font-size: 15px">
                                                                        <i class="icon ion-md-star"></i>
                                                                        <i class="icon ion-md-star"></i>
                                                                        <i class="icon ion-md-star"></i>
                                                                        <i class="icon ion-md-star"></i>
                                                                        <i class="icon ion-md-star-outline empty"></i>
                                                                    </div><!-- Rate -->
                                                                }
                                                                else
                                                                {
                                                                    if (ViewBag.notas > 4 && ViewBag.notas < 5)
                                                                    {
                                                                        <div class="product-rating" style="font-size: 15px">
                                                                            <i class="icon ion-md-star"></i>
                                                                            <i class="icon ion-md-star"></i>
                                                                            <i class="icon ion-md-star"></i>
                                                                            <i class="icon ion-md-star"></i>
                                                                            <i class="icon ion-md-star-half"></i>
                                                                        </div><!-- Rate -->
                                                                    }
                                                                    else
                                                                    {
                                                                        <div class="product-rating" style="font-size: 15px">
                                                                            <i class="icon ion-md-star"></i>
                                                                            <i class="icon ion-md-star"></i>
                                                                            <i class="icon ion-md-star"></i>
                                                                            <i class="icon ion-md-star"></i>
                                                                            <i class="icon ion-md-star"></i>
                                                                        </div><!-- Rate -->
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                            <p>@ViewBag.quantnotas</p>
                        </div>
                        <p style="    margin-bottom: 65px;"><strong>Disponibilidade:</strong>(@ViewBag.P.Quantidade)</p>

                        <div class="product-btns d-sm-flex">
                            <form action="" class="mb-2">
                                <button class="secundary-btn add-to-cart">Comprar Agora</button>
                            </form>
                            <form action="">
                                <button class="primary-btn add-to-cart teste" type="button" onclick="window.location.href='@Url.Action("AddCart", "Home", new { id = ViewBag.P.Id })'"><i class="icon ion-md-cart"></i> Adicionar ao Carrinho</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /Product Details -->
        </div>
        <!-- /row -->
    </div>
    <!-- /container -->
</section>
<!-- /PRODUCT VIEW -->
<!-- DISCRETION -->
<section>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="section-title">
                    <h4 class="title">Descrição</h4>
                </div>
            </div>
            <div class="col-md-12 text-justify">
